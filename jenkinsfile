pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git url: "https://github.com/iam-ganeshjadhav/E-Commerce-Spring-Boot-Application.git", branch: 'main'
            }
        }

        stage('Show Files') {
            steps {
                sh 'ls -R .'
            }
        }
    }
}
